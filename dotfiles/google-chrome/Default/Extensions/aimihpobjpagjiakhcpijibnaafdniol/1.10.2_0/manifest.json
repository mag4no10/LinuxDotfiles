{
   "applications": {
      "gecko": {
         "id": "ezpp@bittipiilo.com"
      }
   },
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "*://osu.ppy.sh/*" ]
   } ],
   "description": "Calculate pp for a beatmap directly in your browser.",
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ezpp!",
   "page_action": {
      "default_icon": "icon48.png",
      "default_popup": "popup.html"
   },
   "permissions": [ "tabs", "storage", "https://*.ppy.sh/", "http://*.ppy.sh/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.10.2"
}
